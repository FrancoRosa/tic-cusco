ault();\n    console.log(image)\n    if (image) {\n      console.log('saving...')\n      setImages([...images, image]);\n      setImage(null);\n    }\n  }\n\n  return(\n  <div className=\"new\">\n    <h1>Nuevo Producto</h1>\n    <form action=\"\">\n      <h6>Descripcion</h6>\n      <input type=\"text\" value={description} onChange={e=> setDescription(e.target.value)} />\n      \n      <h6>Categorias</h6>\n      <p>{categories.join(', ')}</p>\n      <input type=\"text\" value={category}\n        onChange={e=> setCategory(e.target.value)}\n        placeholder=\"teclado\"\n      />\n      <button onClick={addCategory}>A√±adir categoria</button>\n      \n      <h6>Stock</h6>\n      <input type=\"number\" value={stock} onChange={e=> setStock(e.target.value)} />\n      \n      <h6>Precio</h6>\n      <input type=\"number\" value={price} onChange={e=> setPrice(e.target.value)} />\n      \n      <h6>Destacar</h6>\n      <input type=\"checkbox\" value={highlight} onChange={e=>setHighlight(e.target.checked)} />\n      \n      <h6>Listar</h6>\n      <input type=\"checkbox\" value={list} onChange={e=> setList(e.target.checked)} />\n      \n      <h6>Imagenes</h6>\n      {images.map(img => <img src={URL.createObjectURL(img)} alt=\"\"/>)}\n      <input type=\"file\" onChange={e=> setImage(e.target.files[0])} />\n      <button onClick={addImage}>A√±adir Imagen</button>\n      <br />\n\n      <button onClick={saveProduct}>Guardar</button>\n    </form>\n    <button onClick={()=>history.push('/dashboard')}>Cancelar</button>\n  </div>\n)};\n\nexport default AdminNew;",{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":24,"column":6,"nodeType":"97","endLine":24,"endColumn":8,"suggestions":"98"},{"ruleId":"99","severity":1,"message":"100","line":39,"column":19,"nodeType":"101","messageId":"102","endLine":39,"endColumn":21},{"ruleId":"99","severity":1,"message":"103","line":43,"column":18,"nodeType":"101","messageId":"102"