';\nimport AdminLogin from './AdminLogin';\nimport AdminDashboard from './AdminDashboard';\nimport AdminProduct from './AdminProduct';\nimport AdminNew from './AdminNew';\nimport AdminEdit from './AdminEdit';\nimport { auth } from '../firebase';\nimport { setProducts, setUser } from '../actions';\nimport { db } from '../firebase';\nimport Footer from './material/Footer';\nimport ProductPage from './material/ProductPage';\n\n\n\n\nconst App = ({ user, setUser, products, setProducts }) => {\n  \n  const getProducts = () => {\n    console.log('... reading products from Firebase')\n    const allProducts = [];\n    db.collection('products').get()\n    .then(query => {\n      console.log('... saving data on redux store')\n      query.forEach(doc => allProducts.push({ ...doc.data(), id: doc.id }));\n      console.log(allProducts)\n      setProducts(allProducts);\n    })\n    .catch(error => {\n      console.error(error)\n    })\n  }\n  \n  useEffect(() => {\n    auth.onAuthStateChanged(authUser => {\n      if (authUser) {\n        setUser(authUser);\n      } else {\n        setUser(null);\n      }\n    });\n    getProducts();\n  }, [])\n\n  return (\n    <Router>\n      <div className=\"app\">\n        <Switch>\n          \n          <Route path=\"/new\">\n            <AdminNew />\n          </Route>\n          \n          <Route path=\"/dashboard/undefined\">\n            <AdminDashboard />\n          </Route>\n\n          <Route path=\"/dashboard/:id\">\n            <AdminProduct />\n          </Route>\n\n          <Route path=\"/edit/:id\">\n            <AdminEdit />\n          </Route>\n\n          <Route path=\"/dashboard\">\n            <AdminDashboard />\n          </Route>\n\n          <Route path=\"/admin\">\n            <AdminLogin />\n          </Route>\n          \n          <Route path=\"/login\">\n            <Login />\n          </Route>\n          \n          <Route path=\"/checkout\">\n            <Header user={user}/>\n            <Checkout />\n          </Route>\n          \n          <Route path=\"/payment\">\n            <Header user={user}/>\n            <Payment />\n          </Route>\n\n          <Route path=\"/product/:id\">\n            <Header user={user} />\n            <ProductPage />\n            <Footer/>\n          </Route>\n\n          <Route path=\"/\">\n            <Header user={user} />\n            <Home />\n            <Footer/>\n          </Route>\n\n        </Switch>\n      </div>\n    </Router>\n  );\n}\nconst mapStateToProps = state => ({\n  user: state.user,\n  products: state.products,\n});\n\nconst mapDispatchToProps = dispatch => ({\n  setUser: user => dispatch(setUser(user)),\n  setProducts: products => dispatch(setProducts(products)),\n})\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/home/fx/Upwork/tic-cusco/src/firebase.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/index.js",[],"/home/fx/Upwork/tic-cusco/src/components/Login.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/Checkout.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/Payment.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/AdminLogin.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/AdminDashboard.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/AdminNew.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/AdminProduct.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/AdminEdit.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/material/Header.jsx",["119","120"],"/home/fx/Upwork/tic-cusco/src/components/material/Home.jsx",[],"/home/fx/Upwork/tic-cusco/src/actions/index.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/products.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/user.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/admin.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/basket.js",[],"/home/fx/Upwork/tic-cusco/src/reducers/filter.js",[],"/home/fx/Upwork/tic-cusco/src/components/Subtotal.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/BasketItem.jsx",[],"/home/fx/Upwork/tic-cusco/src/compon