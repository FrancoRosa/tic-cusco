rk/tic-cusco/src/components/material/Footer.jsx",[],"/home/fx/Upwork/tic-cusco/src/components/material/ProductPage.jsx",["117"],"import { connect } from \"react-redux\"\nimport { useHistory, useParams } from \"react-router-dom\";\nimport \"react-responsive-carousel/lib/styles/carousel.min.css\"; // requires a loader\nimport { Carousel } from 'react-responsive-carousel';\nimport { useEffect, useState } from \"react\";\nimport './css/ProductPage.css';\nimport { Button } from \"@material-ui/core\";\nimport AddIcon from '@material-ui/icons/Add';\nimport { addToBasket } from '../../actions';\nimport ShoppingCart from '@material-ui/icons/ShoppingCart';\nimport StorefrontIcon from '@material-ui/icons/Storefront';\n\nconst ProductPage = ({ products, addToBasket }) => {\n  const params = useParams();\n  const history = useHistory();  \n  const [product, setProduct] = useState({\n    id: '',\n    title: '',\n    description: '',\n    brand: '',\n    categories: [],\n    stock: 0,\n    price: 0,\n    urls: [],\n  })\n  \n  const updateProduct = products => {\n    if (products.length>0) {\n      const product = products.filter(product => product.id === params.id)[0];\n      const {\n        id,\n        title,\n        description,\n        brand,\n        categories,\n        stock,\n        price,\n        urls,\n      } = product;\n      setProduct({\n        id,\n        title,\n        description,\n        brand,\n        categories,\n        stock,\n        price,\n        urls,\n      });\n    }\n  }\n\n  useEffect(()=>{\n    updateProduct(products)\n  }, [products])\n\n  return (\n    <div className=\"product__page\">\n      <Carousel infiniteLoop autoPlay\n        showStatus={false}\n        transitionTime={1000}\n        interval={8000}\n      >\n          {product.urls.map(url=> <div className=\"productpage__img\"><img src={url} alt=\"\"/></div>)}\n      </Carousel>\n      <div className=\"\">\n        <div className=\"\">\n          <table className=\"table product__table\">\n            <tbody>\n              <tr><th>Titulo:</th> <td>{product.title}</td></tr>\n              <tr><th>Descripcion:</th> <td>{product.description}</td></tr>\n              <tr><th>Marca:</th> <td>{product.brand}</td></tr>\n              <tr><th>Categorias ({product.categories.length}):</th> <td>{product.categories.join(', ')}</td></tr>\n              <tr><th>Stock:</th> <td>{product.stock}</td></tr>\n              <tr><th>Precio:</th> <td>{product.price}</td></tr>\n            </tbody>\n          </table>\n          <div className=\"productpage__actions\">\n            <Button\n              startIcon={<AddIcon />}\n              style={{textTransform: 'none'}}\n              variant=\"outlined\" color=\"black\"\n              onClick={() => addToBasket(product)}\n            >\n              Añadir al carrito\n            </Button>\n            <Button\n              startIcon={<StorefrontIcon />}\n              style={{textTransform: 'none'}}\n              variant=\"outlined\" color=\"black\"\n              onClick={() => history.push('/')}\n            >\n              Regresar al catalogo\n            </Button>\n            <Button\n              startIcon={<ShoppingCart />}\n              style={{textTransform: 'none'}}\n              variant=\"outlined\" color=\"black\"\n              onClick={() => history.push('/checkout')}\n            >\n              Ver carrito\n            </Button>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nconst mapStateToProps = state => ({\n  products: state.products\n});\n\nconst mapDispatchToProps = dispatch => ({\n  addToBasket: item => dispatch(addToBasket(item)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ProductPage);\n",{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":50,"column":6,"nodeType":"124","endLine":50,"endColumn":8,"suggestions":"125"},{"ruleId":null,"fatal":true,"sever